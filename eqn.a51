ORG 0000H ; (U+3Z - 6X)/6D where D = 03H, X = 02H, U = 25H and Z = 12H
	MOV R0,#03H
	MOV R1,#02H
	MOV R2,#25H
	MOV R3,#12H
	MOV A,R3
	MOV B,#3H
	MUL AB
	MOV 30H,A
	MOV A,R1
	MOV B,#06H
	MUL AB
	MOV 31H,A
	MOV A,R0
	MOV B,#6H
	MUL AB
	MOV 32H,A
	MOV A,R2
	MOV B,30H
	ADD A,B
	MOV 33H,A
	MOV A,33H
	MOV B,31H
	SUBB A,B
	MOV 34H, A
	MOV A, 34H
	MOV B, 32H
	DIV AB
	MOV 35H,A
	MOV 36H,B
	END
	